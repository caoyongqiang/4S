<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.CarOwners" table="car_owners">
		<id column="id" name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="name" length="50" type="string"/>
		<property name="phoneNumber" length="50" type="string"/>
		<property name="idCard" length="18" type="string"/>
		<property name="house" length="500" type="string"/>
		<property name="purchaseTime" length="23" type="timestamp"/>
		<property name="car" length="50" type="string"/>
		<property name="carPrice" length="53" type="float"/>
		<property name="plateNumber" length="30"  type="string"/>
		<property name="other" length="53" type="float"/>
		<property name="totalize" length="53" type="float"/>
	</class>
</hibernate-mapping>
