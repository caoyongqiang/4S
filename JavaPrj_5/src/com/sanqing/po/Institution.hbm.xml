<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.Institution" table="institution">
		<id column="id" name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="name" length="50"  type="string"/>
		<property name="phoneNumber" length="20" type="string"/>
		<property name="idCard" length="18" type="string"/>
		<property name="house" length="500" type="string"/>
		<property name="desireCar" length="500" type="string"/>
		<property name="visitTime" length="23" type="timestamp"/>
	</class>
</hibernate-mapping>
