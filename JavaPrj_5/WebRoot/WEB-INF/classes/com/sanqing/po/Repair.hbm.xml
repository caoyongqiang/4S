<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.Repair" table="repair">
		<id column="id" name="id" type="long">
			<generator class="native"/>		<!-- 主键生成策略 -->
		</id>
		<property name="name" length="50" type="string"/>
		<property name="RepairTimes" length="10" type="integer"/>
		<property name="repair" length="50" type="string"/>
		<property name="car" length="100" type="string"/>
		<property name="plateNumber" length="50" type="string"/>
		<property name="repairType" length="50" type="string"/>
		<property name="repairCost" length="50" type="string"/>
		<property name="tel" length="50" type="string"/>
		<property name="email" length="50" type="string"/>
		<property name="createtime" length="23" type="timestamp"/>
		<property name="repairDetail" length="1073741823" type="string"/>
		<property name="isstock" length="1" type="byte"/>
	</class>
</hibernate-mapping>
