<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sanqing.po.Service" table="service">
		<id column="id" name="id" type="long">
			<generator class="native"/>		<!-- 主键生成策略 -->
		</id>
		<property name="name" length="100" type="string"/>
		<property name="phoneNumber" length="20" type="string"/>
		<property name="requireDetail" length="2000" type="string"/>
		<property name="car" length="50" type="string"/>
		<property name="plateNumber" length="50" type="string"/>
		<property name="createtime" length="23" type="timestamp"/>
		<property name="service" length="1" type="byte"/>
		<property name="summarize" length="2000" type="string"/>
	</class>
</hibernate-mapping>
